//Lab 4, Part 2: Kilometers to Miles Conversion with Loop
public class MToKm {
	
	public static void main(String[] args){
		
		int miles= 1;
		double km = 1.609;
		
		System.out.println("Kilometers \t Miles");
		for(miles = 1; miles<=10; miles++){
			System.out.printf("%d \t\t %.3f \n", miles, (miles*km));
		}
	}

}
